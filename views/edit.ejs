<h1>Edit <%= event.name %>
</h1>
<form action="/new" method="POST">
    <div><label for="name">Event Name</label>
        <input type="text" name="name" id="name" placeholder="Event Name">
    </div>
    <div><label for="description">description</label><br>
        <textarea name="description" id="description" cols="30" rows="10"></textarea>
    </div>
    <div><label for="start-time">Start Time</label>
        <input type="datetime-local" name="start-time" id="start-time" value="<%= event.startTime %>">
    </div>
    <div><label for="end-time">End Time</label>
        <input type="datetime-local" name="end-time" id="end-time">
    </div>

    <input type="submit" value="Save">
</form>

<a href="/">All Events</a><a href="">Revert</a>
<form action="/<%= event._id %>/edit" method="post"></form>