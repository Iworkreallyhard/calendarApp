<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Calendar View</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
</head>

<body>
    <main class="container">
        <div class="row">
            <input type="month" name="month" id="month-picker">
        </div>
        <div class="row">
            <div class="day-header col">Monday</div>
            <div class="day-header col">Tuesday</div>
            <div class="day-header col">Wednesday</div>
            <div class="day-header col">Thursday</div>
            <div class="day-header col">Friday</div>
            <div class="day-header col">Saturday</div>
            <div class="day-header col">Sunday</div>
        </div>

        <% let count=1 %>

            <% for( let i=1; i<=7; i++) { %>
                <div class="row">
                    <% for( let j=1; j<=7; j++) { %>
                        <div class="col day">
                            <!-- <%= count %>
                                <% count++ %> -->
                        </div>

                        <% } %>
                </div>

                <% } %>

    </main>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
        crossorigin="anonymous"></script>
    <script src="js/calendarView.js"></script>
</body>

</html>